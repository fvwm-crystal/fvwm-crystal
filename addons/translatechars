#!/bin/sh

if [ "$1" != "$(echo "$1" | tr '[\!\-(){}]\?]\^\`\=\|\&\%\#\*\\\@\"\|\,\;\:]' '_')" ]; then
    mv "$1" "$(echo $1 | tr '[\!\-(){}]\?]\^\`\=\|\&\%\#\*\\\@\"\|\,\;\:]' '_')";
fi

